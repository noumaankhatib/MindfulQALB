# MindfulQALB Backend Environment Variables
# Copy this file to .env and fill in the values

# Server Configuration
PORT=3001
NODE_ENV=development

# Frontend Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000,https://mindfulqalb.com

# Cal.com Configuration
CALCOM_API_KEY=cal_live_xxxxxxxxxxxxx
CALCOM_USERNAME=mindfulqalb
CALCOM_EVENT_TYPE_IDS={"individual":"12345","couples":"12346","family":"12347","free":"12348"}

# Razorpay Configuration
RAZORPAY_KEY_ID=rzp_live_xxxxxxxxxxxxx
RAZORPAY_KEY_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Stripe Configuration
STRIPE_SECRET_KEY=sk_live_xxxxxxxxxxxxx
STRIPE_WEBHOOK_SECRET=whsec_xxxxxxxxxxxxx
STRIPE_SUCCESS_URL=https://mindfulqalb.com/booking/success
STRIPE_CANCEL_URL=https://mindfulqalb.com/booking/cancel

# Security
ENCRYPTION_KEY=your-32-character-encryption-key-here

# Database (for production - optional for development)
# DATABASE_URL=postgresql://user:password@host:5432/mindfulqalb
